<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:test="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zhibaowang.ui.ScanBillActivity">




    <ImageView
        android:id="@+id/back_upload"
        android:layout_width="33dp"
        android:layout_height="21dp"
        android:layout_gravity="left|top"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/arrow"
        android:onClick="onBackClick" />



    <FrameLayout
        android:id="@+id/framelayout_scan_bill_center"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="56dp"
        android:layout_marginTop="38dp"
        android:padding="15dp">


    </FrameLayout>

    <FrameLayout
        android:id="@+id/framelayout_scan_bill_bottom"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom"
        android:layout_marginBottom="0dp"
        android:padding="0dp"></FrameLayout>

    <FrameLayout
        android:id="@+id/framelayout_progress_scan_bill"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">

        <ProgressBar
            android:id="@+id/progress_scan_bill"
            style="@style/progressbar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center" />
    </FrameLayout>
</FrameLayout>
